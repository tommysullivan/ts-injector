{
  "clusters":[
    {
      "id": "clusterID",
      "name": "clusterID.devops.lab",
      "nodes": [
        {
          "name": "node.node1IP",
          "username": "${node1userName}",
          "password": "${node1password}",
          "host": "node1IP",
          "operatingSystem": {
            "name": "${node1OS}",
            "version": "${node1Version}"
          },
          "serviceNames": [
            "mapr-core",
            "mapr-fileserver",
            "mapr-nodemanager",
            "mapr-zookeeper",
            "mapr-cldb",
            "mapr-resourcemanager",
            "mapr-hbase",
            "mapr-webserver",
            "mapr-historyserver",
            "mapr-opentsdb",
            "mapr-grafana",
            "mapr-collectd"
          ]
        }
      ]
    }
  ]
}