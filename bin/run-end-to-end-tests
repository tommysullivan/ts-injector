#!/bin/bash
export clusterUnderTestId=${clusterUnderTestId:-tommy-1-node-centos-7-0-cluster}
export phase=${phase:-latestBuild}
set -e
echo $phase
echo $clusterUnderTestId
clusterUnderTestId=${clusterUnderTestId} phase=${phase} bin/run-cukes --tags @ESXI
clusterUnderTestId=${clusterUnderTestId} phase=${phase} bin/run-cukes --tags @mapr-setup
clusterUnderTestId=${clusterUnderTestId} phase=${phase} bin/run-cukes --tags @rest-installer