{
  "name": "private-devops-automation-framework",
  "version": "6.4.0",
  "description": "collection of libraries for testing and automating infrastructure asynchronously",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "devops": "bin/devops",
    "private-devops-automation-framework": "bin/devops"
  },
  "scripts": {
    "start": "bin/devops",
    "devops": "bin/devops",
    "test": "portalId=lab bin/devops run cucumber -- --tags @regression",
    "localTest": "bin/devops run cucumber -- --tags @regression",
    "build": "rm -rdf dist && tsc",
    "devRefresh": "rm -rdf node_modules/private-devops-* && npm install private-devops-configuration",
    "tsc": "rm -rdf dist && tsc -w",
    "cucumber": "cucumber.js --require dist/support --require dist/step-definitions",
    "mesosSetup": "bin/setup-mesos-slaves.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "features/*",
    "ats-files/*",
    "data/topologies/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapr/private-devops-automation-framework.git"
  },
  "keywords": [
    "spyglass"
  ],
  "author": "Tommy Sullivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapr/private-devops-automation-framework/issues"
  },
  "homepage": "https://github.com/mapr/private-devops-automation-framework#readme",
  "dependencies": {
    "@types/node": "^7.0.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cucumber": "^1.3.1",
    "cucumber-tsflow": "^1.1.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.0",
    "node-uuid": "^1.4.7",
    "nodemiral": "^1.1.1",
    "private-devops-configuration": "^2.3.2",
    "promise": "^7.1.1",
    "readline-sync": "^1.4.1",
    "request": "^2.72.0",
    "scp2": "^0.4.0",
    "shell-escape": "^0.2.0",
    "source-map-support": "^0.4.0",
    "valid-url": "^1.0.9",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "typescript": "^2.1.1"
  },
  "publishConfig": {
    "registry": "http://artifactory.devops.lab/artifactory/api/npm/local-npm-mapr/"
  }
}
