{
  "name": "private-devops-automation-framework",
  "version": "2.2.2",
  "description": "collection of libraries for testing and automating infrastructure asynchronously",
  "main": "./dist/index.js",
  "typings": "./dist/lib/index.d.ts",
  "bin": {
    "devops": "bin/devops"
  },
  "scripts": {
    "start": "bin/devops",
    "test": "npm run build && portalId=lab clusterId=ubuntu14 bin/devops run cucumber -- --tags @regression",
    "localTest": "npm run build && clusterId=ubuntu14 cucumber.js --require dist/support --require dist/step-definitions --tags @regression",
    "build": "rm -rdf dist && tsc"
  },
  "files": [
    "dist/*",
    "bin/*",
    "features/*",
    "ats-files/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapr/private-devops-automation-framework.git"
  },
  "keywords": [
    "spyglass"
  ],
  "author": "Tommy Sullivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapr/private-devops-automation-framework/issues"
  },
  "homepage": "https://github.com/mapr/private-devops-automation-framework#readme",
  "dependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cucumber": "^0.10.2",
    "cucumber-tsflow": "^1.1.1",
    "moment": "^2.15.0",
    "node-uuid": "^1.4.7",
    "nodemiral": "^1.1.1",
    "private-devops-configuration": "^1.0.0",
    "promise": "^7.1.1",
    "readline-sync": "^1.4.1",
    "request": "^2.72.0",
    "scp2": "^0.4.0",
    "shell-escape": "^0.2.0",
    "source-map-support": "^0.4.0",
    "valid-url": "^1.0.9",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "typescript": "^1.8.7"
  },
  "publishConfig": {
    "registry": "http://artifactory.devops.lab/artifactory/api/npm/local-npm-mapr/"
  }
}
